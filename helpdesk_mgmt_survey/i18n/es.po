#. module: helpdesk_mgmt_survey
#: model:mail.template,body_html:helpdesk_mgmt_survey.survey_email_template
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>Hello ${object.partner_id.name},</p>\n"
"        <p>We would like to get your feedback on the support.</p>\n"
"        <hr/>\n"
"        <p>You can rate the service provided in this <a href=\"${object.survey_url}\">survey</a>.</p>\n"
"        <p>Thank you.</p>\n"
"      </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>Hello ${object.partner_id.name},</p>\n"
"        <p>Nos gustaría recibir su opinión sobre el soporte.</p>\n"
"        <hr/>\n"
"        <p>Puede valorar el servicio prestado en esta <a href=\"${object.survey_url}\">encuesta</a>.</p>\n"
"        <p>Gracias.</p>\n"
"      </data>"

#. module: helpdesk_mgmt_survey
#: model:mail.template,body_html:helpdesk_mgmt_survey.closed_survey_ticket_template
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>Hello ${object.partner_id.name},</p>\n"
"        <p>Your ticket has been resolved and closed.</p>\n"
"        <hr/>\n"
"        <p>We would like to get your feedback on the support.</p>\n"
"        <p>You can rate the service provided in this <a href=\"${object.survey_url}\">survey</a>.</p>\n"
"        <p>Thank you.</p>\n"
"      </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>Hello ${object.partner_id.name},</p>\n"
"        <p>Your ticket has been resolved and closed.</p>\n"
"        <hr/>\n"
"        <p>We would like to get your feedback on the support.</p>\n"
"        <p>You can rate the service provided in this <a href=\"${object.survey_url}\">survey</a>.</p>\n"
"        <p>Thank you.</p>\n"
"      </data>"

