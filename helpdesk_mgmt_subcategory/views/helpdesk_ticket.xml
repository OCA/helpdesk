    <field name="inherit_id" ref="helpdesk_mgmt.helpdesk_ticket_view_search" />
    <field name="arch" type="xml">
      <group>
        <filter
            string="Category"
            name="group_category"
            domain="[]"
            context="{'group_by':'category_id'}"
        />
      </group>
    </field>
  </record>
