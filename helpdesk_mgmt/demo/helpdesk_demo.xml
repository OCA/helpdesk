    <record id="helpdesk_ticket_6" model="helpdesk.ticket">
        <field
            eval="&quot;Problem with the delivery of assignments&quot;"
            name="name"
        />
        <field
            eval="&quot;Problem with the delivery of assignments&quot;"
            name="description"
        />
        <field eval="&quot;1&quot;" name="priority" />
